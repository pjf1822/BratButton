{
  "expo": {
    "name": "Brat Button",
    "slug": "BitchButton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brat.jpg",
    "scheme": "bratbutton",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/brat-button.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#E9B4DB"
    },
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "com.pjf1822.BratButton"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "bratButton"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.pjf1822.BitchButton"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/brat.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d60ee586-adbe-425a-898b-f4ff0e7ebd66"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d60ee586-adbe-425a-898b-f4ff0e7ebd66"
    }
  }
}
